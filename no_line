/* ************************************************************************** */ /* */ /* ::: :::::::: */ /* main.c :+: :+: :+: */ /* +:+ +:+ +:+ */ /* By: ncolomer <ncolomer@student.42.fr> +#+ +:+ +#+ */ /* +#+#+#+#+#+ +#+ */ /* Created: 2019/11/11 16:29:19 by ncolomer #+# #+# */ /* Updated: 2019/11/14 19:20:21 by ncolomer ### ########.fr */ /* */ /* ************************************************************************** */ #include <stdio.h> #include <unistd.h> #include <stdlib.h> #include "get_next_line.h" int main(void) { int r; char *line; line = NULL; r = 0; while ((line = get_next_line(r)) > 0) { if (line) { printf("%s", line); free(line); } line = NULL; } if (line) { printf("%s", line); free(line); } line = NULL; }